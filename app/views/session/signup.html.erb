<%= form_with(url: '/register', method: 'post') do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :password_confirm %>
    <%= f.password_field :password_confirm %>
    <%= f.submit :register %>
<% end %>